<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key><string>local.obsidian.planserver</string>

  <key>ProgramArguments</key>
  <array>
    <string>/opt/miniconda3/bin/conda</string>
    <string>run</string>
    <string>-n</string>
    <string>base</string>
    <string>uvicorn</string>
    <string>obs_plan_server:app</string>
    <string>--host</string><string>127.0.0.1</string>
    <string>--port</string><string>8787</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/Mac/obsidian-plan-server</string>

  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><true/>

  <key>StandardOutPath</key><string>/tmp/planserver.out.log</string>
  <key>StandardErrorPath</key><string>/tmp/planserver.err.log</string>
</dict>
</plist>
